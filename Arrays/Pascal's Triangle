class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        res = [[1]*i for i in range(1,numRows+1)]
        if numRows == 1 or numRows == 2:
            return res
        j = 1
        for i in range(2,numRows):#2,3,4
            for j in range(1,i):
                res[i][j] = res[i-1][j-1] + res[i-1][j]
        return res
